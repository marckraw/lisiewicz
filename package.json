{
  "name": "mrck.dev",
  "private": true,
  "scripts": {
    "typescript:lint": "tsc --project tsconfig.json --dry",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "dev": "next dev",
    "clean:run:dev": "yarn clean:all && yarn dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "serve:export:production": "yarn clean:all && yarn build && yarn export && npx http-serve ./out",
    "clean:all": "rm -rf .next && rm -rf ./out",
    "kill:next": "npx kill-port 3000",
    "netlify:dev": "netlify dev",
    "netlify:build": "netlify build",
    "netlify:build:dry": "yarn build && netlify build",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "next": "^12.1.6",
    "react": "17",
    "react-dom": "17",
    "sass": "^1.51.0",
    "storyblok-js-client": "^4.5.2",
    "storyblok-react": "^0.1.2"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.2.5",
    "@next/bundle-analyzer": "^12.1.6",
    "@types/gtag.js": "^0.0.10",
    "@types/node": "^17.0.33",
    "@types/react": "17",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
