{
  "name": "mrck.dev",
  "private": true,
  "scripts": {
    "typescript:lint": "tsc --project tsconfig.json --dry",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "dev": "next dev",
    "clean:run:dev": "yarn clean:all && yarn dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "serve:export:production": "yarn clean:all && yarn build && yarn export && npx http-serve ./out",
    "clean:all": "rm -rf .next && rm -rf ./out",
    "kill:next": "npx kill-port 3000",
    "netlify:dev": "netlify dev",
    "netlify:build": "netlify build",
    "netlify:build:dry": "yarn build && netlify build",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "^12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.44.0",
    "storyblok-js-client": "^4.1.5",
    "storyblok-react": "^0.1.2"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.0.0-rc.1",
    "@next/bundle-analyzer": "^12.0.7",
    "@types/gtag.js": "^0.0.8",
    "@types/node": "^16.11.12",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "axios": "^0.24.0",
    "eslint": "7",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.5.1",
    "typescript": "4.5.2"
  }
